00002e e050      	ldi silencio, 0
00002f e060      	ldi cont, 0
000030 e020      	ldi sonar, 0
000031 e939      	ldi sec1, 0b10011001
000032 e646      	ldi sec2, 0b01100110
000033 dfe6      	rcall conf_timer1
000034 9478      	SEI
         	
