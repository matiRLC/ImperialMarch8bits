          isr_oc1a:
000007 930f         push temp
000008 b70f      	in temp, sreg
000009 930f      	push temp
00000a 1761      	cp cont, cont10ms   ;compara con el tiempo de espera deseado
00000b f011      	breq pasaron10ms
00000c 9563      	inc cont            ;sino, incrementa el cont de entradas
00000d c008         rjmp salir_isr
         
          pasaron10ms:
00000e e060      	ldi cont, 0			  ;reiniciamos el contador
00000f 3020      	cpi sonar, 0        ;es el silencio de la nota o del silencio en si?
000010 f009      	breq silencio_sonar
000011 c002         rjmp silencio_silencio
         
          silencio_sonar:     ;sonar = 0
000012 e051      	ldi silencio, 1
000013 c002      	rjmp salir_isr
         	
          silencio_silencio:  ;sonar = 1
000014 e050      	ldi silencio, 0	
000015 c000      	rjmp salir_isr
         	
          salir_isr:	
000016 910f      	pop temp
000017 bf0f      	out sreg, temp
000018 910f      	pop temp
000019 9518      	reti
         
         ;******************************************************************************
         ;******************************************************************************
         ;**********************     CONFIGURACION TIMER1    ***************************
         ;******************************************************************************
         ;******************************************************************************
         ;                           Modo CTC con OCR1A
         ;                 0<<WGM13 1<<WGM12  0<<WGM11 0<<WGM10
         ;
         ;                          Preescalador = 1
         ;                       0<<CS12 0<<CS11 1<<CS10
         ;
         ;                      interrupciones habilitadas
         ;                              1<<OCIE1A
         ;
         ;                    generador de ondas habilitado
         ;                          0<<COM1A1 1<<COM1A0
         
          conf_timer1:
00001a 930f      	push temp
00001b e400      	ldi temp, (0<<COM1A1)|(1<<COM1A0)|(0<<WGM11)|(0<<WGM10)
00001c bd0f      	out TCCR1A, temp
00001d e009      	ldi temp, (0<<WGM13) |(1<<WGM12) | (1<<CS10)
00001e bd0e      	out TCCR1B, temp
00001f e100      	ldi temp, 1<<OCIE1A
000020 bf09      	out TIMSK, temp
000021 910f      	pop temp
000022 9508      	ret	
         	
